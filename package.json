{
  "name": "redux-playground",
  "version": "1.0.0",
  "description": "A place to learn redux",
  "main": "index.js",
  "scripts": {
    "old start": "webpack-dev-server --config webpack.config.dev.js --port 3000",
    "serve": "webpack serve --config webpack.config.dev.js --progress",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stujmar/redux-playground.git"
  },
  "author": "Stuart Marsh",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/stujmar/redux-playground/issues"
  },
  "homepage": "https://github.com/stujmar/redux-playground#readme",
  "dependencies": {
    "bootstrap": "^5.0.1",
    "immer": "^9.0.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.4",
    "react-router-dom": "^5.2.0",
    "react-toastify": "^7.0.4",
    "redux": "^4.1.0",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "babel-preset-react-app": "^10.0.0",
    "css-loader": "^5.2.6",
    "cssnano": "^5.0.4",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "eslint": "^7.27.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.23.3",
    "eslint-plugin-react": "^7.23.2",
    "fetch-mock": "^9.11.0",
    "html-webpack-plugin": "^5.3.1",
    "http-server": "^0.12.3",
    "jest": "^27.0.1",
    "json-server": "^0.16.3",
    "mini-css-extract-plugin": "^1.6.0",
    "node-fetch": "^2.6.1",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^5.3.0",
    "react-test-renderer": "^17.0.2",
    "react-testing-library": "^8.0.1",
    "redux-immutable-state-invariant": "^2.1.0",
    "redux-mock-store": "^1.5.4",
    "rimraf": "^3.0.2",
    "style-loader": "^2.0.0",
    "webpack": "^5.38.1",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-cli": "^4.7.0",
    "webpack-dev-server": "^3.11.2"
  },
  "engines": {
    "node": ">=8"
  },
  "babel": {
    "presets": [
      "babel-preset-react-app"
    ]
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:import/errors",
      "plugin:import/warnings"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "env": {
      "browser": true,
      "node": true,
      "es6": true,
      "jest": true
    },
    "rules": {
      "no-debugger": "off",
      "no-console": "off",
      "no-unused-vars": "warn",
      "react/prop-types": "warn"
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    },
    "root": true
  }
}
